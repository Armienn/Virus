<Window x:Class="VirusNetwork.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
		<CheckBox x:Name="MasterCheckbox" Content="Master" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
		<Label Content="IP" HorizontalAlignment="Left" Margin="10,31,0,0" VerticalAlignment="Top"/>
		<Label Content="Port" HorizontalAlignment="Left" Margin="10,62,0,0" VerticalAlignment="Top"/>
		<TextBox x:Name="IpBox" HorizontalAlignment="Left" Height="23" Margin="54,33,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
		<TextBox x:Name="PortBox" HorizontalAlignment="Left" Height="23" Margin="54,64,0,0" TextWrapping="Wrap" Text="3000" VerticalAlignment="Top" Width="120"/>
		<Button x:Name="StartButton" Content="Start" HorizontalAlignment="Left" Margin="208,34,0,0" VerticalAlignment="Top" Width="75" Click="StartButton_Click"/>
		<TextBlock x:Name="InTextBox" HorizontalAlignment="Left" Margin="13,93,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="217" Width="161"/>

	</Grid>
</Window>
